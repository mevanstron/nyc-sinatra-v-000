<h1>Update Landmark</h1>
<form action="/landmarks/<%= @landmark.id %>" method="post">
  <input id="hidden" type="hidden" name="_method" value="patch">
  <label for="name">Name: </label><input id="name" type="text" name="landmark[name]" value="<%= @landmark.name %>">

  <br></br>

  <label for="landmark_year_completed">Name: </label><input id="landmark_year_completed" type="text" name="landmark[year_completed]" value="<%= @year_completed %>">

  <br></br>

  <button type="submit" name="edit" value="Edit Figure">Edit Figure</button>
</form>
